{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js" 
      }
    ],
    "env": {
      "MY_SECRET": "@my-secret", 
      "PORT": 3000,
      "ERASE_DATABASE_ON_SYNC": "true"
    }
  }
  